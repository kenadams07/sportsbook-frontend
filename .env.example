# Database Configuration
DB_HOST=postgres
DB_PORT=5433
DB_USERNAME=postgres
DB_PASSWORD=your_password_here
DB_NAME=sportsbook
DB_AUTO_LOAD_ENTITIES=true
DB_SYNCHRONIZE=true

# Redis Configuration
REDIS_URL=redis://redis:6379

# RabbitMQ Configuration
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=guest
RABBITMQ_URL=amqp://${RABBITMQ_DEFAULT_USER}:${RABBITMQ_DEFAULT_PASS}@rabbitmq:5672
RABBITMQ_QUEUE_USER=user_queue
RABBITMQ_QUEUE_ADMIN=admin_queue
RABBITMQ_QUEUE_ODDS=odds_queue

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
TOKEN_EXPIRESIN=24h

# Service Ports
USER_SERVICE_PORT=3001
ADMIN_SERVICE_PORT=3002
MATCH_ODDS_SERVICE_PORT=3003
SOCKET_PORT=4000

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DB=sportsbook

# pgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=your_pgadmin_password_here